{
  "name": "Laradock",
	"dockerComposeFile": [
		"../../laradock/docker-compose.yml"
	],
  "remoteUser": "laradock",
  "runServices": [
    "apache2",
    "mysql",
    "redis",
    "phpmyadmin",
    "php-fpm"
  ],
  "service": "workspace",
  "workspaceFolder": "/var/www",
  "shutdownAction": "stopCompose",
  "postCreateCommand": "uname -a"
}
